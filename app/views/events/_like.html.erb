<!--いいねの非同期--><!--パーシャル化-->
<div id="event_<%= event.id %>">
  <% if current_user.liked_by?(event.id) %>
    <p>
      <%= link_to destroy_like_path(event.id), class: "like-link", method: :DELETE, remote: true do %>
        <img class="like-btn" src="https://illustimage.com/photo/6052.png" alt="いいね" >
      <% end %>
      <%= event.likes.count %>
    </p>
  <% else %>
    <p>
      <%= link_to create_like_path(event.id), class: "like-link", method: :POST, remote: true do %>
        <img class="unlike-btn" src="data:image/png;base64," alt="いいねを外す" >
      <% end %>
      <%= event.likes.count %>
    </p>
  <% end %>
</div>
